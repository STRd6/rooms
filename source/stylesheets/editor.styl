.itemPalette
  position absolute
  left 0
  top 0

  overflow hidden
  width 128px
  z-index 1

.itemContainer
  border-radius 4px
  background-color white
  display inline-block
  margin 4px
  padding 4px

.trash
  background-image url("/images/dumper.png")
  background-size 100%
  position absolute
  bottom 0
  right 0

  width 100px
  height 100px
